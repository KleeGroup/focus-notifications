{
    "name": "focus-notifications",
    "version": "0.4.0-beta6",
    "description": "Notification Center extension for focus",
    "main": "index.js",
    "scripts": {
        "babelify": "better-npm-run babelify",
        "build": "better-npm-run babelify && better-npm-run copy-sass && better-npm-run bundle",
        "bundle": "better-npm-run bundle",
        "copy-sass": "better-npm-run copy-sass",
        "lint": "eslint src",
        "prepublish": "npm run build",
        "start": "better-npm-run dev-server",
        "test": "better-npm-run test"
    },
    "betterScripts": {
        "bundle": {
            "command": "webpack --progress",
            "env": {
                "DEV": false,
                "NODE_ENV": "production",
                "BABEL_ENV": "production",
                "LIBRARY_NAME": "focus-notifications",
                "MINIMIFY": false
            }
        },
        "copy-sass": {
            "command": "cd src && find . -name *.scss | xargs tar cvf - | tar xfp - -C ../"
        },
        "babelify": {
            "env": {
                "BABEL_ENV": "production"
            },
            "command": "node script/babelify.js"
        },
        "dev-server": {
            "command": "node ./server.js",
            "env": {
                "DEV": true,
                "SOURCE_MAP": false,
                "ENTRY_FILE_PATH": "./src/example/index",
                "PAGE_TITLE": "Focus notifications dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "ANCHOR_CLASS": "notification-center-example"
            }
        },
        "test": {
            "command": "node ./node_modules/mocha/bin/mocha src/**/__tests__/**/*.js"
        }
    },
    "babel": {
        "presets": [
            "focus"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/get-focus/focus-notifications.git"
    },
    "keywords": [
        "notification",
        "focus"
    ],
    "author": "Focus by Kleegroup <focus@kleegroup.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/get-focus/focus-notifications/issues"
    },
    "homepage": "https://github.com/get-focus/focus-notifications#readme",
    "files": [
        "__tests__",
        "actions",
        "api-mock",
        "component",
        "config.js",
        "container",
        "dist",
        "example",
        "index.html",
        "index.js",
        "LICENSE",
        "package.json",
        "README.md",
        "reducers",
        "src",
        "server.js",
        "store",
        "style",
        "test",
        "util",
        "webpack.config.js"
    ],
    "dependencies": {
        "isomorphic-fetch": "^2.1.1",
        "redux-devtools": "^3.3.1",
        "redux-devtools-dock-monitor": "^1.1.1",
        "redux-devtools-log-monitor": "^1.2.0"
    },
    "peerDependencies": {
        "i18next": ">= 4.1.0",
        "lodash": ">= 4.15.0",
        "material-design-lite": ">= 1.2.1",
        "material-design-icons-iconfont": ">= 3.0.3",
        "react": ">= 15.3.0 < 16.0.0",
        "react-dom": ">= 15.3.0 < 16.0.0",
        "react-redux": ">= 4.4.5",
        "redux": ">= 3.5.2",
        "redux-thunk": ">= 2.1.0",
        "moment": ">= 2.14.1"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-preset-focus": "^0.5.2",
        "better-npm-run": "0.0.5",
        "babel": "^6.0.15",
        "babel-cli": "^6.4.0",
        "babel-core": "^6.1.2",
        "babel-eslint": "^4.1.4",
        "babel-loader": "^6.1.0",
        "babel-runtime": "^6.0.14",
        "body-parser": "^1.14.1",
        "chai": "^3.3.0",
        "chai-subset": "^1.1.0",
        "css-loader": "^0.16.0",
        "eslint": "1.5.1",
        "eslint-config-focus": "^0.3.0",
        "eslint-plugin-filenames": "0.1.1",
        "eslint-plugin-react": "3.5.0",
        "express": "^4.13.3",
        "extract-text-webpack-plugin": "^0.8.2",
        "faker": "^3.0.1",
        "file-loader": "^0.8.4",
        "jsdom": "^6.5.1",
        "json-loader": "^0.5.2",
        "lodash": "^4.15.0",
        "material-design-icons-iconfont": "^3.0.3",
        "material-design-lite": "1.2.1",
        "mocha": "^2.3.0",
        "moment": "^2.14.1",
        "nock": "^2.17.0",
        "node-sass": "^3.3.3",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.1",
        "react-addons-test-utils": "0.14.4",
        "react-hot-loader": "^1.2.7",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "redux-logger": "^2.7.4",
        "sass-loader": "^3.1.2",
        "sinon": "^1.17.1",
        "sinon-chai": "^2.8.0",
        "source-map-loader": "^0.1.5",
        "style-loader": "^0.13.0",
        "url-loader": "^0.5.6",
        "webpack": "^1.9.6",
        "webpack-dev-server": "^1.8.2",
        "webpack-focus": "^0.11.5"
    }
}
